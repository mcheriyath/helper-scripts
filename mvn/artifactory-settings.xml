<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <servers>
        <server>
            <username>username@example.com</username>
            <password>artifactoryapikey</password>
            <id>central</id>
        </server>
        <server>
            <username>username@example.com</username>
            <password>artifactoryapikey</password>
            <id>snapshots</id>
        </server>
        <server>
            <username>username@example.com</username>
            <password>artifactoryapikey</password>
            <id>maven-local</id>
        </server>
        <server>
            <username>username@example.com</username>
            <password>artifactoryapikey</password>
            <id>maven-local-snapshots</id>
        </server>
    </servers>
    <profiles>
      <profile>
        <repositories>
          <repository>
              <snapshots>
                  <enabled>false</enabled>
              </snapshots>
              <id>maven-local</id>
              <name>maven-local</name>
              <url>https://artifactory.example.com/artifactory/maven-local</url>
          </repository>
          <repository>
              <snapshots/>
              <id>maven-local-snapshots</id>
              <name>maven-local-snapshots</name>
              <url>https://artifactory.example.com/artifactory/maven-local</url>
          </repository>
          <repository>
            <snapshots>
              <enabled>false</enabled>
            </snapshots>
            <id>central</id>
            <name>maven</name>
            <url>https://artifactory.example.com/artifactory/maven</url>
          </repository>
          <repository>
            <snapshots />
            <id>snapshots</id>
            <name>maven</name>
            <url>https://artifactory.example.com/artifactory/maven</url>
          </repository>
        </repositories>
        <pluginRepositories>
          <pluginRepository>
            <snapshots>
              <enabled>false</enabled>
            </snapshots>
            <id>central</id>
            <name>maven</name>
            <url>https://artifactory.example.com/artifactory/maven</url>
          </pluginRepository>
          <pluginRepository>
            <snapshots />
            <id>snapshots</id>
            <name>maven</name>
            <url>https://artifactory.example.com/artifactory/maven</url>
          </pluginRepository>
        </pluginRepositories>
        <id>artifactory</id>
      </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>
